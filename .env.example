# FilmLens - Environment Variables
# NO SE MODIFICA , ESTE ES UN EJEMPLO PARA EL EQUIPO
# SE DEBE CREAR UN ARCHIVO .env LOCAL CON LOS VALORES REALES

# General
PROJECT_NAME=filmlens-trigger-map
ENVIRONMENT=development


# Docker
USER_ID=1000
GROUP_ID=1000


# Jupyter Lab
JUPYTER_TOKEN=filmlens
JUPYTER_PORT=8888


# PostgreSQL Database
POSTGRES_USER=filmlens_user
POSTGRES_PASSWORD=filmlens_dev_2025
POSTGRES_DB=filmlens
POSTGRES_PORT=5432

# Database URL for applications
DATABASE_URL=postgresql://filmlens_user:filmlens_dev_2025@localhost:5432/filmlens


# Flyway Database Migrations
FLYWAY_URL=jdbc:postgresql://database:5432/filmlens
FLYWAY_USER=filmlens_user
FLYWAY_PASSWORD=filmlens_dev_2025
FLYWAY_SCHEMAS=public
FLYWAY_BASELINE_VERSION=0
FLYWAY_BASELINE_ON_MIGRATE=true


# MLflow Tracking
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_EXPERIMENT_NAME=filmlens_triggers
MLFLOW_PORT=5000
MLFLOW_S3_BUCKET=filmlens-mlflow-artifacts


# API Configuration

API_HOST=0.0.0.0
API_PORT=8000
DEBUG=true
LOG_LEVEL=info


# External API Keys

TMDB_API_KEY=your_tmdb_api_key_here
OMDB_API_KEY=your_omdb_api_key_here


# AWS/S3 (for DVC remote storage)

AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1
DVC_REMOTE_BUCKET=your-s3-bucket-name
S3_BUCKET=filmlens-data


# Model Configuration

MODEL_PATH=/app/models/production
MODEL_VERSION=v1.0-baseline
